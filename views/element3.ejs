<script>
    const team1 = document.getElementById("team1");
    const team2 = document.getElementById("team2");

    function showImage1(){
    team1.style.display="block";
    setTimeout(()=>{
         team1.style.display="none";
    },3000)
    }

    function showImage2(){
    team2.style.display="block";
    setTimeout(()=>{
         team2.style.display="none";
    },3000)
    }
</script>
 

